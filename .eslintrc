{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "parserOptions": {
        "ecmaFeatures": {
            "jsx":true
        }
    },
  "root": true,
  "rules": {
    "prefer-const": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "no-else-return": 0,
    "no-bitwise": 0,
    "no-underscore-dangle": 0,
    "no-nested-ternary": 0,
    "no-plusplus": 0,
    "no-mixed-operators": 0,
    "no-unused-expressions": 0,
    "func-names": 0,
    "indent":0,
    "eqeqeq":0,
    "no-sequences":0,
    "comma-dangle": [0, "never"],
    "comma-spacing":0,
    "key-spacing":0,
    "block-spacing":0,
    "block-scoped-var": 0,
    "quotes":0,
    "no-use-before-define":0,
    "no-multi-spaces":0,
    "object-curly-spacing":0,
    "semi":0,
    "keyword-spacing":0,
    "space-before-blocks":0,
    "arrow-spacing":0,
    "spaced-comment":0,
    "no-console":0,
    "space-before-function-paren":0,
    "vars-on-top":0,
    "space-in-parens":0,
    "padded-blocks":0,
    "consistent-return":0,
    "no-extra-semi":0,
    "one-var":0,
    "max-len":0,
    "no-tabs":0,
    "object-shorthand":0,
    "no-shadow":0,
    "semi-spacing":0,
    "arrow-body-style":0,
    "prefer-arrow-callback":0,
    "prefer-template":0,
    "global-require":0,
    "space-infix-ops":0,
    "generator-star-spacing":0,
    "arrow-parens":0,
    "prefer-rest-params":0,
    "quote-props":0,
    "array-bracket-spacing":0,
    "one-var-declaration-per-line":0,
    "no-lonely-if":0,
    "template-curly-spacing":0,
    "eol-last":0,
    "radix":0,
    "strict":0,
    "yoda":0,
    "no-return-assign":0,
    "new-parens":0,
    "new-cap":0,
    "require-yield":0,
    "no-trailing-spaces":0,
    "no-unneeded-ternary":0,
    "guard-for-in":0,
    "no-restricted-syntax":0,
    "no-useless-escape":0,
    "no-multiple-empty-lines":0,
    "no-extend-native":0,
    "import/imports-first":0,
    "import/no-mutable-exports":0,
    "import/newline-after-import":0,
    "import/prefer-default-export":0
   }
}
